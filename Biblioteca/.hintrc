{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "@-ms-viewport"
        ]
      }
    ],
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 120",
    "not and_ff <= 119",
    "not chrome <= 120",
    "not edge <= 120",
    "not firefox <= 121",
    "not ios_saf <= 17.2",
    "not opera <= 106",
    "not safari <= 17.2",
    "not samsung <= 23"
  ]
}